# Environment Variables Template
# Copy this file to .env and fill in your actual values

# Database
MONGO_URI=mongodb://localhost:27017/travelapp

# Authentication
JWT_SECRET=your_jwt_secret_here

# Server
PORT=4002

# Email Configuration
EMAIL_USER=your_email@gmail.com
EMAIL_PASS=your_email_password

# OpenRouter API for Qwen Integration
OPENROUTER_API_KEY=your_openrouter_api_key_here

# Amadeus API Credentials
AMADEUS_API_KEY=your_amadeus_api_key
AMADEUS_API_SECRET=your_amadeus_api_secret
AMADEUS_API_URL=https://test.api.amadeus.com/v1

# Skyscanner API Credentials
SKYSCANNER_API_KEY=your_skyscanner_api_key
SKYSCANNER_API_URL=https://partners.api.skyscanner.net/apiservices

# Booking.com API Credentials
BOOKING_API_KEY=your_booking_api_key
BOOKING_API_URL=https://booking-com.p.rapidapi.com/v1

# Hertz API Credentials
HERTZ_API_KEY=your_hertz_api_key
HERTZ_API_URL=https://api.hertz.com/v1

# Stripe Payment Processing
STRIPE_SECRET_KEY=sk_test_your_stripe_test_key_here